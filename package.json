{
  "name": "@txstate-mws/svelte-components",
  "version": "0.0.4",
  "description": "Svelte components that are generically useful.",
  "svelte": "dist-esm/index.js",
  "module": "dist-esm/index.js",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "svelte-check": "svelte-check --ignore 'dist,dist-esm'",
    "build": "npm run svelte-check && rm -rf dist* && tsc && tsc -m commonjs --outDir dist && rm -rf dist/tests && cp -R src/components/*.svelte dist/components/ && rm -rf dist-esm/tests && cp -R src/components/*.svelte dist-esm/components/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:txstate-etc/svelte-components.git"
  },
  "author": "Nick Wing",
  "license": "MIT",
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "rfdc": "^1.2.0",
    "tabbable": "^5.1.5",
    "txstate-utils": "^1.2.26"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "@storybook/svelte": "^6.1.15",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-svelte3": "^3.0.0",
    "jest": "^26.6.3",
    "svelte": "^3.32.1",
    "svelte-check": "^1.1.31",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.6.5",
    "ts-jest": "^26.5.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ]
    },
    "transformIgnorePatterns": [],
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ]
  },
  "sideEffects": false,
  "files": [
    "dist",
    "dist-esm",
    "docs"
  ]
}
