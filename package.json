{
  "name": "@txstate-mws/svelte-components",
  "version": "1.0.0",
  "description": "Svelte components that are generically useful.",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "svelte": "esm/index.js",
  "types": "dist/ts/index.d.ts",
  "scripts": {
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "svelte-check": "svelte-check --ignore 'dist'",
    "prebuild": "rm -rf ./dist",
    "build": "npm run svelte-check && tsc -m commonjs && node scripts/preprocess.js && rm -rf dist/tests",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:txstate-etc/svelte-components.git"
  },
  "author": "Nick Wing",
  "license": "MIT",
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "rfdc": "^1.2.0",
    "tabbable": "^5.1.5",
    "txstate-utils": "^1.4.0"
  },
  "devDependencies": {
    "@storybook/svelte": "^6.2.9",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^13.1.9",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^15.3.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-jest": "^27.3.1",
    "eslint": "^7.18.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.6",
    "jest": "^27.3.1",
    "svelte": "^3.37.0",
    "svelte-check": "^2.1.0",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.6.5",
    "ts-jest": "^27.0.7",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ]
    },
    "transformIgnorePatterns": [],
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ]
  },
  "sideEffects": false,
  "files": [
    "dist",
    "esm",
    "docs"
  ]
}
