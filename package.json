{
  "name": "@txstate-mws/svelte-components",
  "version": "1.0.0",
  "description": "Svelte components that are generically useful.",
  "svelte": "src/index.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "dev": "rollup -c -w",
    "svelte-check": "svelte-check",
    "build": "rollup -c",
    "prepublishOnly": "npm run svelte-check && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@git.txstate.edu:mws/txmantic-svelte.git"
  },
  "author": "Nick Wing",
  "license": "MIT",
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "rfdc": "^1.2.0",
    "txstate-utils": "^1.2.26"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "@storybook/svelte": "^6.1.15",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-svelte3": "^3.0.0",
    "jest": "^26.6.3",
    "rollup": "^2.38.3",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.32.1",
    "svelte-check": "^1.1.31",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.6.5",
    "ts-jest": "^26.5.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ]
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ]
  }
}
